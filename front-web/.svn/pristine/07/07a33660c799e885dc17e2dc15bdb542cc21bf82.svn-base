.o2_mini .entry_1 .entry_item {
	width: 290px
}
.o2_mini .entry_1 .entry_item_7, .o2_mini .entry_1 .entry_item_8 {
	width: 190px
}
.o2_mini .entry_1 .live {
	width: 390px
}
.entry_1 .entry_item_7, .entry_1 .entry_item_8 {
	width: 190px;
	height: 170px
}
.entry_1 .entry_item_7 .entry_bg, .entry_1 .entry_item_8 .entry_bg {
	width: 100%;
	height: 80px;
	bottom: 0;
	left: 0;
	top: auto
}
.entry_1 .entry_item_7 .entry_info, .entry_1 .entry_item_8 .entry_info {
	padding-top: 0;
	width: 100%;
	padding: 10px 0 0;
	text-align: center
}
.entry_1 .entry_item_7 .entry_info_tit, .entry_1 .entry_item_8 .entry_info_tit {
	font-size: 16px;
	line-height: 34px;
	width: 100px;
	height: 30px;
	margin: 0 auto
}
.entry_1 .entry_item_7 .entry_info_desc, .entry_1 .entry_item_8 .entry_info_desc {
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden
}
.entry_1 .entry_item_7 .entry_img, .entry_1 .entry_item_8 .entry_img {
	display: block;
	width: 200px;
	height: 170px
}
.entry_c3 .entry_list {
	height: 530px
}
.entry_c3 .live {
	float: right;
	height: 350px;
	color: #fff;
	font-size: 12px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none
}
.live_inner {
	width: 100%;
	height: 100%;
	background: #333
}
.live_hd {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 38px;
	background: rgba(0,0,0,.57);
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000)
}
.live_hd_lk {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 38px;
	line-height: 38px;
	color: #fff
}
.live_hd_lk:hover {
	color: #fff
}
.live_hd_lk_tit {
	line-height: 38px;
	text-indent: 24px;
	font-size: 16px;
	font-weight: 400
}
.live_hd_lk_more {
	position: absolute;
	right: 20px;
	top: 0;
	height: 38px;
	line-height: 38px;
	color: #fff;
	padding-right: 24px
}
.live_hd_ico {
	position: absolute;
	right: 0;
	top: 11px;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px -128px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 15px;
	height: 16px
}
.live_status {
	position: absolute;
	right: 20px;
	top: 45px;
	height: 20px;
	line-height: 20px;
	padding: 0 10px;
	background: rgba(0,0,0,.46);
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#66000000, endColorstr=#66000000);
	-moz-border-radius: 10px;
	border-radius: 10px
}
.live_status_led {
	position: relative;
	float: left;
	width: 8px;
	height: 8px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	background: #fd3131;
	top: 6px;
	margin: 0 4px 0 -4px
}
.live_playback .live_status_led {
	display: none
}
.live_btn {
	position: absolute;
	top: 145px;
	background: #2d2d2d;
	background: rgba(0,0,0,.42);
	width: 20px;
	height: 48px;
	line-height: 48px;
	color: #fff;
	text-align: center
}
.live_btn_pre {
	left: 0
}
.live_btn_next {
	right: 0
}
.live_btn:hover {
	color: #ee3232
}
.live_playbtn {
	position: absolute;
	left: 50%;
	top: 50%;
	margin: -33px 0 0 -33px;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: 0 -105px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 65px;
	height: 65px;
	z-index: 2
}
.live_player {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: #333;
	display: block
}
.live_item_mask, .live_mask {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: #000;
	opacity: .37;
	filter: alpha(opacity=37)
}
.live_mask {
	display: none
}
.live_item {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%
}
.live_img {
	height: 350px;
	margin-left: -38px
}
.live_cover {
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden
}
.live_cover:after {
	content: "";
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: url(data:image/png;base64,) repeat-x 0 100%
}
.live_ind {
	position: absolute;
	left: 0;
	top: 325px;
	width: 100%;
	text-align: center;
	line-height: 8px
}
.live_ind_item {
	display: inline-block;
*zoom:1;
*display:inline;
	width: 8px;
	height: 8px;
	background: #fff;
	margin: 0 4px;
	-moz-border-radius: 4px;
	border-radius: 4px
}
.live_ind_item.active {
	background: #fd3131
}
.live_info {
	position: absolute;
	top: 260px;
	width: 100%;
	text-align: center;
	z-index: 5
}
.live_info_name {
	font-size: 16px;
	line-height: 26px
}
.live_info_name_ico {
	display: inline-block;
*zoom:1;
*display:inline;
	position: relative;
	top: 5px;
*top:0;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px -52px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 20px;
	height: 20px;
	margin-right: 4px
}
.live_info_desc {
	font-size: 14px;
	line-height: 24px
}
.live_bar {
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 40px;
	background: rgba(0,0,0,.57);
filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#99000000, endColorstr=#99000000);
	display: none
}
.live_bar_tit {
	float: left;
	line-height: 40px;
	font-size: 16px;
	text-indent: 22px;
	width: 245px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap
}
.live_bar_tit_ico {
	display: inline-block;
*zoom:1;
*display:inline;
	position: relative;
	top: 4px;
*top:0;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px -52px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 20px;
	height: 20px;
	margin-right: 4px
}
.live_volume {
	position: absolute;
	right: 18px;
	top: 12px;
	width: 88px;
	height: 16px
}
.live_volume_ico {
	left: 0;
	top: 0;
	display: block;
	position: absolute;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px -92px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 16px;
	height: 16px
}
.live_volume_bar {
	position: absolute;
	right: 0;
	top: 5px;
	width: 60px;
	height: 6px;
	overflow: hidden;
	-moz-border-radius: 3px;
	border-radius: 3px;
	background: #6a6a6a;
	cursor: pointer
}
.live_volume_bar_inner {
	width: 100%;
	height: 100%;
	background: url(data:image/png;base64,) #fff
}
.live_tip {
	position: absolute;
	background: #4d4d4d;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	text-align: center;
	display: none
}
.live_tip_img {
	margin: 106px auto 0
}
.live_tip_txt {
	margin-top: 13px;
	font-size: 14px;
	line-height: 30px;
	color: #999
}
.live_tip_lk {
	color: #fff;
	text-decoration: underline
}
.live_tip_lk:hover {
	color: #fff
}
.live_tip_lk_ico {
	display: inline-block;
*zoom:1;
*display:inline;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px -128px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 15px;
	height: 16px;
	position: relative;
	left: 3px;
	top: 3px;
*top:0
}
.live_disabled .live_playbtn {
	z-index: 6
}
.live_disabled_lk {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 6;
	display: none
}
.live_disabled .live_disabled_lk {
	display: block
}
.video-js {
	width: 100%;
	height: 100%
}
.live_hd {
	z-index: 10
}
.live_status {
	z-index: 10
}
.live_playbtn {
	z-index: 10
}
.live_btn {
	z-index: 10
}
.live_info {
	z-index: 10
}
.live_ind {
	z-index: 7
}
.live_bar {
	z-index: 7
}
.live_item_mask {
	z-index: 6
}
.live_mask {
	z-index: 6
}
.live_tip {
	z-index: 6
}
.live_player {
	z-index: -1
}
.live_playing .live_hd {
	display: none
}
.live_playing .live_status {
	right: 15px;
	top: 10px
}
.live_playing .live_status_led {
	-webkit-animation: live_status_led_blink 1s ease infinite;
	-moz-animation: live_status_led_blink 1s ease infinite;
	animation: live_status_led_blink 1s ease infinite
}
.live_playing .live_item_mask {
	display: none
}
.live_playing .live_mask {
	display: block;
	opacity: 0;
	filter: alpha(opacity=1)
}
.live_playing .live_playbtn {
	display: none
}
.live_playing .live_btn {
	display: none
}
.live_playing .live_info {
	display: none
}
.live_playing .live_bar {
	display: block
}
.live_playing .live_bar_ico {
	display: block
}
.live_playing .live_ind {
	display: none
}
.live_playing .live_player {
	z-index: 5
}
.live_playing:hover .live_hd {
	display: block
}
.live_playing:hover .live_status {
	right: 20px;
	top: 45px
}
.live_playing:hover .live_btn {
	display: block
}
@-webkit-keyframes live_status_led_blink {
0%, 100% {
opacity:0
}
50% {
opacity:1
}
}
@-moz-keyframes live_status_led_blink {
0%, 100% {
opacity:0
}
50% {
opacity:1
}
}
@keyframes live_status_led_blink {
0%, 100% {
opacity:0
}
50% {
opacity:1
}
}
.live_paused .live_item_mask {
	display: none
}
.live_paused .live_mask {
	display: block
}
.live_paused .live_btn {
	display: block
}
.live_paused .live_playbtn {
	display: block;
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -85px -105px;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 65px;
	height: 65px
}
.live_paused .live_player {
	z-index: 5
}
.live_paused .live_info {
	display: none
}
.live_paused .live_bar {
	display: block
}
.live_paused .live_ind {
	display: none
}
.live_error .live_player_bar {
	display: block
}
.live_error .live_item_mask {
	display: none
}
.live_error .live_mask {
	display: none
}
.live_error .live_btn {
	display: block
}
.live_error .live_playbtn {
	display: none
}
.live_error .live_info {
	display: none
}
.live_error .live_bar {
	display: block
}
.live_error .live_ind {
	display: none
}
.live_error .live_tip {
	display: block
}
.live_error .live_tip_img {
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -105px 0;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 80px;
	height: 80px
}
.live_end .live_player_bar, .live_onroad .live_player_bar {
	display: block
}
.live_end .live_item_mask, .live_onroad .live_item_mask {
	display: none
}
.live_end .live_mask, .live_onroad .live_mask {
	display: none
}
.live_end .live_btn, .live_onroad .live_btn {
	display: block
}
.live_end .live_playbtn, .live_onroad .live_playbtn {
	display: none
}
.live_end .live_info, .live_onroad .live_info {
	display: none
}
.live_end .live_bar, .live_onroad .live_bar {
	display: block
}
.live_end .live_ind, .live_onroad .live_ind {
	display: none
}
.live_end .live_tip, .live_onroad .live_tip {
	display: block
}
.live_end .live_tip_txt {
	margin-top: 140px
}
.live_onroad .live_tip_img {
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: 0 0;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 85px;
	height: 85px
}
.live_loading .live_mask {
	display: none
}
.live_loading .live_btn {
	display: block
}
.live_loading .live_playbtn {
	display: none
}
.live_loading .live_info {
	display: none
}
.live_loading .live_bar {
	display: block
}
.live_loading .live_ind {
	display: none
}
.live_loading .live_tip {
	display: block
}
.live_loading .live_tip_img {
	background-image: url(../images/sprite_entrylive@1x.png);
	background-position: -205px 0;
	-moz-background-size: 237px 170px;
	background-size: 237px 170px;
	background-repeat: no-repeat;
	width: 32px;
	height: 32px;
	margin-top: 140px;
	-webkit-animation: live_tip_img_rotate 1.5s linear infinite;
	-moz-animation: live_tip_img_rotate 1.5s linear infinite;
	animation: live_tip_img_rotate 1.5s linear infinite
}
@-webkit-keyframes live_tip_img_rotate {
0% {
-webkit-transform:rotate(0);
transform:rotate(0)
}
100% {
-webkit-transform:rotate(1turn);
transform:rotate(1turn)
}
}
@-moz-keyframes live_tip_img_rotate {
0% {
-moz-transform:rotate(0);
transform:rotate(0)
}
100% {
-moz-transform:rotate(1turn);
transform:rotate(1turn)
}
}
@keyframes live_tip_img_rotate {
0% {
-webkit-transform:rotate(0);
-moz-transform:rotate(0);
transform:rotate(0)
}
100% {
-webkit-transform:rotate(1turn);
-moz-transform:rotate(1turn);
transform:rotate(1turn)
